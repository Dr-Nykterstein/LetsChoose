{
  "name": "letschoose",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "yarn --cwd server run start",
    "server": "yarn --cwd server run dev",
    "client": "yarn --cwd client run start",
    "client:build": "yarn --cwd client build",
    "heroku-postbuild": "cd client && yarn install --production=false && yarn run build",
    "postinstall": "tsc",
    "dev": "concurrently \"yarn run server\" \"yarn run client\""
  },
  "keywords": [
    "choose",
    "versus",
    "choise",
    "compare"
  ],
  "author": "Rostyslav Begey <rostyk.begey@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "concurrently": "^5.2.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "prettier": "2.0.5",
    "typescript": "^4.0.3"
  }
}
